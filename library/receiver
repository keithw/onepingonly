#!/usr/bin/python

from au_defs import *
import au_recv
from math import *

samples = []
au_recv.open()
for i in range(0,16):
    try:
        samp = au_recv.receive()
        samples.extend(samp)
    except:
        pass

n = 0
for s in samples:
#    print "%d %f" % (n, sqrt(s.real**2 + s.imag**2))
    print "%d %f" % (n, s.real)
    n = n+1
